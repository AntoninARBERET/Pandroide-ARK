@inproceedings{hervieu_trajectory-based_2009,
	address = {New York, NY, USA},
	series = {{CIVR} '09},
	title = {Trajectory-based {Handball} {Video} {Understanding}},
	isbn = {978-1-60558-480-5},
	url = {http://doi.acm.org/10.1145/1646396.1646447},
	doi = {10.1145/1646396.1646447},
	abstract = {This paper presents a content-based approach for understanding handball videos. Tracked players are characterized by their 2D trajectories in the court plane. The trajectories and their interactions are used to model visual semantics, i.e., the observed activity phases. To this end, hierarchical parallel semi-Markov models (HPaSMMs) are computed in order to take into account the temporal causalities of object motions. Players motions are characterized using velocity informations while their interactions are described by the distances between trajectories. We have evaluated our method on real video sequences, and have favorably compared with another method, i.e., hierarchical parallel hidden Markov models (HPaHMMs).},
	urldate = {2019-01-25},
	booktitle = {Proceedings of the {ACM} {International} {Conference} on {Image} and {Video} {Retrieval}},
	publisher = {ACM},
	author = {Hervieu, Alexandre and Bouthemy, Patrick and Cadre, Jean-Pierre Le},
	year = {2009},
	keywords = {computer vision, motion analysis, pattern classification, semi-markov models, sports videos},
	pages = {43:1--43:8},
	file = {ACM Full Text PDF:/Users/antoninarberet/Zotero/storage/V8HYI9RS/Hervieu et al. - 2009 - Trajectory-based Handball Video Understanding.pdf:application/pdf}
}



@article{li_moving_2019,
	title = {Moving {Object} {Detection} in {Video} via {Hierarchical} {Modeling} and {Alternating} {Optimization}},
	volume = {28},
	issn = {1057-7149},
	doi = {10.1109/TIP.2018.2882926},
	abstract = {In conventional wisdom of video modeling, the background is often treated as the primary target and foreground is derived using the technique of background subtraction. Based on the observation that foreground and background are two sides of the same coin, we propose to treat them as peer unknown variables and formulate a joint estimation problem, called Hierarchical modeling and Alternating Optimization (HMAO). The motivation behind our hierarchical extensions of background and foreground models is to better incorporate a priori knowledge about the disparity between background and foreground. For background, we decompose it into temporally low-frequency and high-frequency components for the purpose of better characterizing the class of video with dynamic background; for foreground, we construct a Markov random field prior at a spatially low resolution as the pivot to facilitate the noise-resilient refinement at higher resolutions. Built on hierarchical extensions of both models, we show how to successively refine their joint estimates under a unified framework known as alternating direction multipliers method. Experimental results have shown that our approach produces more discriminative background and demonstrates better robustness to noise than other competing methods. When compared against current state-of-the-art techniques, HMAO achieves at least comparable and often superior performance in terms of F-measure scores, especially for video containing dynamic and complex background.},
	language = {English},
	number = {4},
	journal = {Ieee Transactions on Image Processing},
	author = {Li, Linhao and Hu, Qinghua and Li, Xin},
	month = apr,
	year = {2019},
	note = {WOS:000453552100010},
	keywords = {alternating direction multipliers method (ADMM), background subtraction, dictionary learning, Hierarchical modeling, joint estimation, low-rank, mixture, principal component pursuit, tracking},
	pages = {2021--2036}
}

